<tr>
  <td><%= test.title %></td>
  <td><%= test.level %></td>
  <td><%= test.questions.count %></td>
  <td><%= link_to t('.link_view'), admin_test_path(test), class: 'btn btn-primary' %></td>
  <td><%= link_to t('.link_edit'), edit_admin_test_path(test), class: 'btn btn-info' %></td>
  <td>
    <%= link_to t('.link_delete'), 
                admin_test_path(test),
                method: :delete,
                data: {confirm: t('.delete_confirm')},
                class: 'btn btn-danger' %>
  </td>
</tr>
