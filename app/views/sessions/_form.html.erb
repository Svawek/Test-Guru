<%= form_with url: sessions_path, local: true do |form| %>
  <%= flash_helper('alert') %>
  <p>
    <%= form.label :email %>
    <%= form.text_field :email, id: :email %>
  </p>
  <p>
    <%= form.label :password %>
    <%= form.password_field :password, id: :password %>
  </p>

  <p><%= form.submit 'Enter' %></p>

  <p><%= link_to 'Registration', signup_path %></p>
<% end %>
